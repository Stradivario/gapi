  "bugs": {
    "url": "https://github.com/Stradivario/gapi-apache-kafka/issues"
  },
  "dependencies": {
    "graphql-kafka-subscriptions": "^0.2.2",
    "graphql-subscriptions": "^0.5.8",
    "subscriptions-transport-ws": "^0.9.6"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@gapi/core": "^1.8.23"
